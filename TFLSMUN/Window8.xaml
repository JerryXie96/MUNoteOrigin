<Window x:Class="TFLSMUN.Window8"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="延期动议" Height="196" Width="432" ResizeMode="NoResize" WindowStyle="ThreeDBorderWindow" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="动议人：" Height="27" HorizontalAlignment="Left" Margin="12,62,0,0" Name="label1" VerticalAlignment="Top" Width="54" />
        <TextBox Height="24" HorizontalAlignment="Right" Margin="0,64,250,0" Name="name" VerticalAlignment="Top" Width="88" />
        <Button Content="确定" Height="23" HorizontalAlignment="Left" Margin="312,33,0,0" Name="ok" VerticalAlignment="Top" Width="75" Click="ok_Click" Background="White" BorderBrush="White" Opacity="0.8" />
        <Button Content="取消" Height="23" HorizontalAlignment="Left" Margin="312,80,0,0" Name="cancel" VerticalAlignment="Top" Width="75" Click="cancel_Click" Background="White" BorderBrush="White" Opacity="0.8" />
    </Grid>
</Window>
